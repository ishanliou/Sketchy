<div margin: 10px 0px 0px 0px>
<h1 class="text-center" style="color:#4E4846; margin: 10px 0 10px 0;">Best of Sketchy</h1>
<% sketches.forEach(function(sketch){ %>
    <div class="card">
        <div id="card-thumb" >
                <img class="card-img-left" src="<%= sketch.giphy_url %>" alt="Card image cap">
                <img class="card-img-right" src="<%= sketch.imgUrl %>" alt="Card image cap">
        </div>
   
        <div id="card-desc" class="card-body">
          <h5 class="card-title"><%= sketch.title %></h5>
          <hr>
          <p class="card-text"><%= sketch.tag%></p>
          <h1> <%= sketch%> </h1>
          
        </div>
</div>
<% }) %>
</div>
